#include <iostream>
#include <string>

using namespace std;

class Person
{
public:
  string name;
  int age;
  string lastName;
};

void print(Person* person)
{
  cout << person->name << " is " person->age << " years old" << endl;
}

int main()
{
  Person person;
  person.name = "Harry";
  person.age = 23;
  
  cout << "Meet " << person.name();
  print(&person);
  printf("\n");
  
  return 0;
}
